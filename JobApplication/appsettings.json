{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "MqttSettings": {
    "Broker": "broker.hivemq.com",
    "ClientId": "JobAppClient",
    "Topic": "mytopic/#"
  },
  "JobSettings": {
    "Job1": {
      "Enabled": true,
      "Schedule": "*/5 * * * *" // every 5 minutes
    },
    "Job2": {
      "Enabled": false,
      "Schedule": "0 0 * * *" // every day at midnight
    }
  }
}